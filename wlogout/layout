{
    "label" : "lock",
    "action" : "swaylock -f -C $HOME/.config/swaylock/lock",
    "text" : "Lock",
    "keybind" : "l"
}
{
    "label" : "hibernate",
    "action" : "systemctl hibernate",
    "text" : "Hibernate",
    "keybind" : "h"
}
{
    "label" : "logout",
    "action" : "wlogout -l $HOME/.config/wlogout/logout -p layer-shell -m 360 -c 10 -b 2",
    "text" : "Logout",
    "keybind" : "e"
}
